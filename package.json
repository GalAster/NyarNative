{
    "name": "native-nyar",
    "private": true,
    "scripts": {
        "pest": "cargo test --color=always --test main gen_valkyrie_lexer --no-fail-fast -- --format=json --ignored --exact -Z unstable-options --show-output",
        "build": "cargo build --release",
        "test": "cargo build",
        "fmt": "cargo fmt --all",
        "push": "yarn test && git push",
        "reset": "git reset Head~ --soft",
        "pub": "cargo-publish-all --yes --allow-dirty"
    }
}
