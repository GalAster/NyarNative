namespace core.text;

structure Point {
    x: f64,
    y: f64,
    construct(x: f64, y: f64) {
        self.x = x;
        self.y = y;
    }
}

extends Point {
    sum(self): f64 {
        print_f32(self.x)
        print_f32(self.y)
        add(self.x, self.y)
    }
}








