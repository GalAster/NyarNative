namespace core.text;




structure Point {
    x: f64,
    y: f64,
    construct(x: f64, y: f64) {
        self.x = x;
        self.y = y;
    }
}

extends Point {
    sum(self): f64 {
        add(self.x, self.y)
    }
}


function add(i: f32, j: f32): f32 {
    print_f32(i)
    print_f32(j)
    i + j
}

